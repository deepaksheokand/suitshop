<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   
		<referenceContainer name="content">
			<block class="Magento\Cms\Block\Block" name="home_category_block" after="slider"> 
				<arguments> 
					<argument name="block_id" xsi:type="string">home_category_block</argument> 
				</arguments> 
			</block>
			<block class="Magento\Cms\Block\Block" name="home_faq_bloack" after="bestseller"> 
				<arguments> 
					<argument name="block_id" xsi:type="string">home-faq-block</argument> 
				</arguments> 
			</block>
			<block class="Magento\Cms\Block\Block" name="header_top_left-new" after="home_faq_bloack"> 
					<arguments> 
						<argument name="block_id" xsi:type="string">header-top-left</argument> 
					</arguments> 
				</block>
			<block class="Magento\Cms\Block\Block" name="home-about-us" after="header_top_left-new"> 
				<arguments> 
					<argument name="block_id" xsi:type="string">home-about-us</argument> 
				</arguments> 
			</block>
		</referenceContainer>
		<referenceBlock name="header.container">
			<block class="Magento\Cms\Block\Block" name="mobile-category-menu" after="navigation.sections"> 
					<arguments> 
						<argument name="block_id" xsi:type="string">mobile-category-menu</argument> 
					</arguments> 
				</block>
		</referenceBlock>
  
</page>
